@model IEnumerable<WebPchelki.Models.Entities.Beehive>
@{
    ViewBag.Title = "Ульи";
}

<form asp-action="BeehivePage" asp-controller="Home">
    <a asp-action="BeehiveAdd" asp-controller="Home">Добавить</a>
    <dl class="dl-horizontal">
        @foreach (var item in Model)
        {
            <dt>@item.Name</dt>
            <dd><a asp-action="BeehiveDetails" asp-controller="Home" asp-route-id="@item.Id">Подробнее</a></dd>
        }
    </dl>
</form>
